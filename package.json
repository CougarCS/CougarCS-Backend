{
  "name": "cougarcs-backend",
  "version": "1.0.0",
  "description": "CougarCS-Backend",
  "main": "src/server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=prod babel src -d dist",
    "server": "cross-env NODE_ENV=dev nodemon --exec babel-node ./src/server.js",
    "start": "cross-env NODE_ENV=prod node dist/server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:dev": "cross-env NODE_ENV=test jest --watch --runInBand --detectOpenHandles",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "eslint-check": "eslint ./src --ext .js && eslint ./test --ext .js",
    "eslint-fix": "eslint src/**/*.js --fix && eslint test/*.js --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@notionhq/client": "^0.4.0",
    "@sendgrid/mail": "^7.4.7",
    "@sentry/node": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-actuator": "^1.8.0",
    "express-prom-bundle": "^6.4.1",
    "express-rate-limit": "^5.3.0",
    "express-validator": "^6.12.0",
    "google-spreadsheet": "^3.1.15",
    "googleapis": "^87.0.0",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "prom-client": "^14.0.0",
    "stripe": "^8.175.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/node": "7.15.8",
    "@babel/plugin-transform-runtime": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/register": "7.15.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-prettier-check": "1.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "nodemon": "2.0.13",
    "prettier": "2.4.1",
    "supertest": "6.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/src/server.js",
      "src/utils/api/calls.js",
      "src/utils/logger.js",
      "src/utils/httpLogger.js",
      "src/utils/prometheus.js",
      "/dist/",
      ".eslint.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!data/keyMap.js",
      "!/node_modules/",
      "!/client/"
    ]
  }
}