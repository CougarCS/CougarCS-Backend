{
  "name": "cougarcs-backend",
  "version": "1.1.3",
  "description": "CougarCS-Backend",
  "main": "src/server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=prod babel src -d dist",
    "server": "cross-env NODE_ENV=dev nodemon --exec babel-node ./src/server.js",
    "start": "cross-env NODE_ENV=prod node dist/server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:dev": "cross-env NODE_ENV=test jest --watch --runInBand --detectOpenHandles",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:mutation": "cross-env NODE_ENV=test stryker run",
    "eslint-check": "eslint ./src --ext .js && eslint ./test --ext .js",
    "eslint-fix": "eslint src/**/*.js --fix && eslint test/*.js --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@newrelic/winston-enricher": "^2.1.2",
    "@notionhq/client": "^0.4.2",
    "@sendgrid/mail": "^7.5.0",
    "@sentry/node": "^6.19.0",
    "@sentry/tracing": "^6.19.0",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.16.4",
    "express-actuator": "^1.8.1",
    "express-rate-limit": "^6.2.0",
    "express-validator": "^6.12.2",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "newrelic": "^8.9.0",
    "stripe": "^8.217.0",
    "uuid": "^8.3.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/node": "7.17.10",
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/register": "7.17.7",
    "@changesets/cli": "2.22.0",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@types/jest": "27.4.1",
    "eslint": "8.14.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-prettier-check": "1.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "supertest": "6.2.3"
  }
}