
{
  "name": "cougarcs-backend",
  "version": "1.1.3",
  "description": "CougarCS-Backend",
  "main": "src/server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=prod babel src -d dist",
    "server": "cross-env NODE_ENV=dev nodemon --exec babel-node ./src/server.js",
    "start": "cross-env NODE_ENV=prod node dist/server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:dev": "cross-env NODE_ENV=test jest --watch --runInBand --detectOpenHandles",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "eslint-check": "eslint ./src --ext .js && eslint ./test --ext .js",
    "eslint-fix": "eslint src/**/*.js --fix && eslint test/*.js --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@newrelic/winston-enricher": "^2.1.0",
    "@notionhq/client": "^0.4.2",
    "@sendgrid/mail": "^7.5.0",
    "@sentry/node": "^6.17.1",
    "@sentry/tracing": "^6.17.1",
    "axios": "^0.26.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^11.0.0",
    "express": "^4.16.4",
    "express-actuator": "^1.8.0",
    "express-rate-limit": "^6.2.0",
    "express-validator": "^6.12.2",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "newrelic": "^8.7.1",
    "stripe": "^8.199.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
    "@babel/node": "7.16.8",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/register": "7.17.0",
    "@changesets/cli": "2.21.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-config-prettier-check": "1.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "supertest": "6.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/src/server.js",
      "src/utils/api/",
      "src/utils/caching/",
      "src/utils/logger/",
      "src/utils/monitoring/",
      "src/utils/tracing/",
      "src/utils/config.js",
      "/dist/",
      ".eslint.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!data/keyMap.js",
      "!/node_modules/",
      "!/client/"
    ]
  }
}

